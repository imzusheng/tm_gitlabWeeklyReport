{
  "repositoryUrl": "https://github.com/imzusheng/tm_gitlabWeeklyReport_v2.git",
  "branches": [
    "main",
    "master",
    "v2"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/npm",
      {
        "npmPublish": false
      }
    ],
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "node scripts/semantic-version-update.cjs && npm run build"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "package.json",
          "vite.config.ts",
          "dist/**/*",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "dist/userscript/gitlab-weekly-report.user.js",
            "label": "GitLab Weekly Report Userscript"
          },
          {
            "path": "dist/web/index.html",
            "label": "Web Version"
          }
        ]
      }
    ]
  ]
}