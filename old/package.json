{
  "name": "tm",
  "version": "1.3.0",
  "description": "GitLab 周报生成器 - 使用 DeepSeek AI 的现代化 Tampermonkey 脚本",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run lint:check && webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:release": "npm run release",
    "watch": "webpack --mode=development --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "echo '✅ 提交信息检查通过'"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@eslint/js": "^8.56.0",
    "babel-loader": "^9.1.3",
    "conventional-changelog-cli": "^4.1.0",
    "core-js": "^3.35.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "globals": "^16.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "raw-loader": "^4.0.2",
    "standard-version": "^9.5.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "keywords": [
    "tampermonkey",
    "gitlab",
    "周报",
    "ai",
    "deepseek",
    "userscript"
  ]
}
